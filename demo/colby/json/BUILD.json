{
  "CC": "gcc",
  "CFLAGS": "-I.",
  "OBJ": [
    "hellomake.o",
    "hellofunc.o"
  ],
  "%.o": {
    "deps": [
      "%.c",
      "hellomake.h"
    ],
    "exec": "$(CC) -c -o $@ $< $(CFLAGS)"
  },
  "hellomake": {
    "deps": "$(OBJ)",
    "exec": "$(CC) -o $@ $+ $(CFLAGS)"
  },
  "clean": {
    "exec": "rm -f hellomake $(OBJ)"
  }
}